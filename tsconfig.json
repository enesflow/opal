{
  "compilerOptions": {
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "#/*": [
        "./*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ]
      /* End Frontend */
    },
    "allowJs": true,
    "target": "ES2017",
    "module": "ES2020",
    "lib": [
      "es2020",
      "DOM"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "declaration": true,
    "declarationDir": "lib-types",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true,
    "types": [
      "vite/client"
    ]
  },
  "files": [
    "./.eslintrc.cjs"
  ],
  "include": [
    "src",
  ],
}